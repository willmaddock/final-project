<h1>New Access Log</h1>

<% if flash[:notice] %>
  <div class="alert alert-success" role="alert">
    <%= flash[:notice] %>
  </div>
<% end %>

<% if flash[:alert] %>
  <div class="alert alert-danger" role="alert">
    <%= flash[:alert] %>
  </div>
<% end %>

<%= render "form", access_log: @access_log %>

<div class="mt-3">
  <%= link_to "Back to Access Logs", access_logs_path, class: 'btn btn-secondary' %>
</div>