<% if notice %>
  <div class="alert alert-success" role="alert">
    <%= notice %>
  </div>
<% end %>

<div class="mt-3">
  <%= render @access_log %>

  <div class="mt-3">
    <%= link_to "Edit this Access Log", edit_access_log_path(@access_log), class: 'btn btn-warning me-2' %>
    <%= link_to "Back to Access Logs", access_logs_path, class: 'btn btn-secondary me-2' %>
    <%= button_to "Delete this Access Log", @access_log, method: :delete, data: { confirm: "Are you sure you want to delete this access log?" }, class: 'btn btn-danger' %>
  </div>
</div>